{
  "name": "giveth-dapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bignumber.js": "^4.0.2",
    "create-react-class": "^15.6.0",
    "feathers-authentication-client": "^0.3.3",
    "feathers-client": "^2.3.0",
    "feathers-commons": "^0.8.7",
    "feathers-reactive": "^0.5.2",
    "formsy-react": "^0.19.5",
    "formsy-react-components": "^0.10.1",
    "giveth-react-scripts": "1.0.11",
    "localforage": "^1.5.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-avatar": "^2.3.0",
    "react-dom": "^15.6.1",
    "react-quill": "^1.1.0",
    "react-responsive-masonry": "^1.2.3",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-skylight": "^0.4.2",
    "react-toastify": "^2.0.0-rc.3",
    "sweetalert": "^1.1.3",
    "web3": "0.19.1",
    "web3-eth-accounts": "^1.0.0-beta.18"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
